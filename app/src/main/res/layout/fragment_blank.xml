<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.cn.tfl.fragment.BlankFragment">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/button_no_param_and_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用无参无返回" />

    <Button
        android:id="@+id/button_with_param_no_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用有参无返回" />

    <Button
        android:id="@+id/button_no_param_with_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用无参有返回" />

    <Button
        android:id="@+id/button_with_param_and_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用有参有返回" />
    <Button
        android:id="@+id/button_with_more_param"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调用多参无返回" />

</LinearLayout>
